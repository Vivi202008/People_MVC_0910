@model People_MVC.Models.Person

<table class="table">
    <tbody>       
            <tr>
                <td>@Model.ID</td>
                <td>@Model.City</td>
                <td>@Model.Name</td>
                <td>@Model.TeleNumber</td>
                <td><a asp-action="Delete" asp-route-id="@Model.ID">Delete</a></td>
            </tr>       
    </tbody>
</table>

