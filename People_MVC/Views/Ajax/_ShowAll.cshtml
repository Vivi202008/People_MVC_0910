@model People_MVC.Models.Person

<table class="table">
    <tbody>       
        <tr>
            <th scope="col">@Html.DisplayFor(m => Model.PersonId)</th>
            <th scope="col">@Html.DisplayFor(m => Model.Name)</th>
            <th scope="col">@Html.DisplayFor(m => Model.TeleNumber)</th>
            <th scope="col">@Html.DisplayFor(m => Model.City.Name)</th>
            <th scope="col">@Html.DisplayFor(m => Model.City.Country)</th>
            <th scope="col">@Html.DisplayFor(m => Model.PersonLanguages)</th>
